@model List<AbQuestion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>True Coders</h3>

<div style="display: block">
    <a href=" /Question/InsertQuestion/">Create a new Question</a>
</div>

<table class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
    <thead>
    <tr>
        <th class="th-sm">Angry Bird Question</th>
        <th class="th-sm">Rating</th>
        <th class="th-sm">Category</th>
    </tr>
        </thead>
    @foreach (AbQuestion angry in Model)
    {
        <tr>
            <td><a href=/Question/ViewQuestion/@angry.QuestionID >@angry.AngryBirdQuestion</a></td>
            <td>@angry.RatingID</td>
            <td>@angry.Category</td> 
        </tr>
    }

</table>


