@model List<AbQuestion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Angry Bird Questions</h3>

<table class="table">

    <tr>
        <th>Angry Bird Question</th>
        <th>Question Rating</th>
    </tr>

    @foreach (AbQuestion angry in Model)
    {
        <tr>

            <td><a href=/Question/ViewQuestion/@angry.QuestionID >@angry.AngryBirdQuestion</a></td>
            <td>@angry.RatingID</td>
        </tr>
    }

</table>